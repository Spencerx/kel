kel set-default-cluster //eldarion.cloud
kel resource-groups create eldarion
kel sites create eldarion/hello
kel activate eldarion/hello

kel resource-groups create //eldarion.cloud/eldarion
kel sites create //eldarion.cloud/eldarion/hello
kel activate //eldarion.cloud/eldarion/hello

kel config set cluster=//eldarion.cloud resource-group=eldarion
kel sites create hello
kel activate hello

kel config get uri.host
